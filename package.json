{
  "name": "eslint-plugin-shopify-lean",
  "version": "1.1.2",
  "description": "Trimmed down version of Shopifyâ€™s ESLint rules.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "files": [
    "lib/",
    "index.js"
  ],
  "author": {
    "name": "Sebastian Software",
    "email": "s.werner@sebastian-software.de",
    "url": "https://www.sebastian-software.de"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "release": "git push && release-it --github.release --npm.publish --non-interactive",
    "release:minor": "git push && release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "git push && release-it --github.release --npm.publish --non-interactive --increment major",
    "check": "yarn run lint && yarn test",
    "lint": "eslint . --max-warnings 0 --format codeframe",
    "test": "jest"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "bugs": {
    "url": "https://github.com/sebastian-software/eslint-plugin-shopify-lean/issues"
  },
  "homepage": "https://github.com/sebastian-software/eslint-plugin-shopify-lean",
  "repository": "https://github.com/sebastian-software/eslint-plugin-shopify-lean",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures"
    ],
    "collectCoverage": false
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-jest": "^26.0.1",
    "babel-eslint": "10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-import-resolver-node": "^0.3.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "eslint": "^7.2.0",
    "eslint-plugin-react": "^7.20.0"
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "eslint-module-utils": "^2.6.0",
    "pkg-dir": "4.2.0",
    "pluralize": "^8.0.0"
  }
}
